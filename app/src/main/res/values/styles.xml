<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="colorprimaryCheck">
        <item name="color">@color/colorPrimary</item>
    </style>


    <style name="WhiteCheck">
        <item name="colorAccent">@color/colorPrimary</item>
    </style>

    <style name="SwitchCompatStyle" >
        <item name="thumbTint">@color/switch_color</item>
        <item name="trackTint">@color/switch_color</item>
    </style>

    <style name="MyStyle" parent="AppTheme">
        <item name="android:windowBackground">@drawable/splash</item>
    </style>

</resources>
